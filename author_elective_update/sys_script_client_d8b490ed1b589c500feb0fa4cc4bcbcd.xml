        <name>Form button color</name>
        <order/>
        <script><![CDATA[function onLoad() {

    changeButtonColor('demored', 'red', 'white');
    changeButtonColor('demored_bottom', 'red', 'white');
	


}

function changeButtonColor(buttonID, backgroundColor, buttonTextColor) {
    try {
        $$('button[id=' + buttonID + ']').each(function(elmt) {
            elmt.style.backgroundColor = backgroundColor;
            elmt.style.color = buttonTextColor;
        });
    } catch (e) {}
}]]></script>
